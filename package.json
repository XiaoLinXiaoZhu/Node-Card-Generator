    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/bun": "^1.2.11",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vue-tsc": "^2.2.8"
  }
}
